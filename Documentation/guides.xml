<?xml version="1.0" encoding="UTF-8"?>
<!--
    Configuration for TYPO3 Documentation Rendering
    https://docs.typo3.org/m/typo3/docs-how-to-document/main/en-us/GeneralConventions/DirectoryFilenames.html
-->
<guides
    xmlns="https://www.phpdoc.org/guides"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://www.phpdoc.org/guides vendor/phpdocumentor/guides-cli/resources/schema/guides.xsd"
>
    <project
        title="TYPO3 LLM Extension"
        version="0.1.0"
        release="0.1.0"
    />

    <extension
        class="\T3Docs\Typo3DocsTheme\DependencyInjection\Typo3DocsThemeExtension"
        vendor="TYPO3"
        name="typo3-docs-theme"
    />

    <inventory id="t3coreapi">
        <source url="https://docs.typo3.org/m/typo3/reference-coreapi/main/en-us/"/>
    </inventory>
    <inventory id="t3tca">
        <source url="https://docs.typo3.org/m/typo3/reference-tca/main/en-us/"/>
    </inventory>
    <inventory id="t3tsconfig">
        <source url="https://docs.typo3.org/m/typo3/reference-tsconfig/main/en-us/"/>
    </inventory>
    <inventory id="t3start">
        <source url="https://docs.typo3.org/m/typo3/tutorial-getting-started/main/en-us/"/>
    </inventory>
</guides>
